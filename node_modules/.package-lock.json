  },
  "cookiePopup": {
    "description": "We use cookies to enhance your experience. Manage your preferences below.",
    "title": "Your Privacy Choices",
    "buttons": {
      "acceptAll": "Accept All",
      "customize": "Customize",
      "rejectAll": "Reject Non-Essential",
      "savePreferences": "Save Settings"
    },
    "links": {
      "cookiePolicy": "/cookie-policy",
      "privacyPolicy": "/privacy-policy"
    }
  },
  "cookieSettings": {
    "analytics": {
      "name": "Analytics Cookies",
      "description": "Collect anonymized data for traffic analysis and UX improvements.",
      "isEnabled": false,
      "isRequired": false
    },
    "essential": {
      "name": "Essential Cookies",
      "description": "Required for core functionality (e.g., login, security, language preferences).",
      "isEnabled": true,
      "isRequired": true
    },
    "marketing": {
      "name": "Marketing Cookies",
      "description": "Used to personalize ads based on user interests.",
      "isEnabled": false,
      "isRequired": false
    },
    "preferences": {
      "name": "Preference Cookies",
      "description": "Store UI settings (e.g., dark mode, font size).",
      "isEnabled": false,
      "isRequired": false
    }
  },
  "uiRecommendations": {
    "font": "Roboto, sans-serif",
    "placement": "bottom-right",
    "colors": {
      "background": "#FFFFFF",
      "primary": "#2A5C8D",
      "secondary": "#4CAF50",
      "text": "#333333"
    }
